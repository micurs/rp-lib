{
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http/file-server dist --port 8080 --verbose"
  },
  "imports": {
    "@tailwindcss/typography": "npm:@tailwindcss/typography@^0.5.10",
    "autoprefixer": "npm:autoprefixer@^10.4.16",
    "daisyui": "npm:daisyui@^4.4.20",
    "postcss": "npm:postcss@^8.4.32",
    "postcss-import": "npm:postcss-import@^14.0.0",
    "tailwindcss": "npm:tailwindcss@^3.3.6"
  }
}
